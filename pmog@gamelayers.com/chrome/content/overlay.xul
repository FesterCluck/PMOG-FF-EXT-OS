    </statusbarpanel>
  </statusbar>
  <script>
    <![CDATA[
    jQuery(document).ready(function() {

      jQuery('#pmog-toolbar toolbarbutton').hover(
        function(event) {
          Pmog.onTBBMouseOver(this,event);
        },
        function() {
          Pmog.onTBBMouseOut(this);
        }).bind("mousemove", function() {
          Pmog.onTBBMouseMove(this);
        });

        window.addEventListener("load", function (e) { Pmog.init(); }, false);
      });
    ]]>
  </script>

